{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://redgreen.io/schemas/fluid/manifest.json",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "generator",
    "maintainer"
  ],
  "properties": {
    "generator": {
      "$id": "#/properties/generator",
      "type": "object",
      "additionalProperties": true,
      "required": [
        "name",
        "description",
        "version",
        "id",
        "implementation"
      ],
      "properties": {
        "name": {
          "$id": "#/properties/generator/properties/name",
          "type": "string"
        },
        "description": {
          "$id": "#/properties/generator/properties/description",
          "type": "string"
        },
        "version": {
          "$id": "#/properties/generator/properties/version",
          "type": "string"
        },
        "id": {
          "$id": "#/properties/generator/properties/id",
          "type": "string"
        },
        "implementation": {
          "$id": "#/properties/generator/properties/implementation",
          "type": "string"
        }
      }
    },
    "maintainer": {
      "$id": "#/properties/maintainer",
      "type": "object",
      "additionalProperties": true,
      "required": [
        "name",
        "website",
        "email"
      ],
      "properties": {
        "name": {
          "$id": "#/properties/maintainer/properties/name",
          "type": "string"
        },
        "website": {
          "$id": "#/properties/maintainer/properties/website",
          "type": "string"
        },
        "email": {
          "$id": "#/properties/maintainer/properties/email",
          "type": "string"
        }
      }
    }
  }
}
